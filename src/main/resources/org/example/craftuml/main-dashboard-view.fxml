<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

 <VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.example.craftuml.MainDashboardController">
        <Label text="Craft UML Dashboard" style="-fx-font-size: 24px;"/>
        <Button text="Create Diagram" onAction="#handleCreateDiagram"/>
        <Button text="Export Diagram" onAction="#handleExportDiagram"/>
        <Button text="Generate Code" onAction="#handleGenerateCode"/>
        <Button text="Manage Project" onAction="#handleManageProject"/>
        <Button text="Save Project" onAction="#handleSaveProject"/>
        <Button text="Load Project" onAction="#handleLoadProject"/>
</VBox>
